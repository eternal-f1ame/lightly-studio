{
    "name": "lightly_studio_view",
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "node": ">=22.11.0"
    },
    "scripts": {
        "predev": "npm run generate-api-client",
        "dev": "vite dev",
        "prebuild": "npm run generate-api-client",
        "build": "vite build",
        "preview": "vite preview",
        "check-types": "npx tsc --noEmit",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "npx prettier --write . && eslint . --fix",
        "lint": "npx prettier --check . && eslint .",
        "static-checks": "npm run verify-schema && npm run lint && npm run check-types",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "test:e2e-debug": "playwright test --ui",
        "build-storybook": "storybook build",
        "storybook": "storybook dev -p 6006",
        "get-schema": "openapi-typescript ../lightly_studio/openapi.json -o src/lib/schema.d.ts",
        "verify-schema": "openapi-typescript ../lightly_studio/openapi.json -o src/lib/schema.d.ts --check",
        "generate-api-client": "npm run get-schema && npx @hey-api/openapi-ts"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.2",
        "@hey-api/openapi-ts": "^0.80.1",
        "@lucide/svelte": "^0.482.0",
        "@playwright/test": "^1.53.0",
        "@storybook/addon-essentials": "^8.4.5",
        "@storybook/addon-interactions": "^8.4.5",
        "@storybook/addon-svelte-csf": "^5.0.0-next.11",
        "@storybook/blocks": "^8.4.5",
        "@storybook/svelte": "^8.4.5",
        "@storybook/sveltekit": "^8.4.5",
        "@storybook/test": "^8.4.5",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-static": "^3.0.6",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@testing-library/svelte": "^5.2.6",
        "@types/d3-selection": "^3.0.11",
        "@types/d3-zoom": "^3.0.8",
        "@types/eslint": "^9.6.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.15",
        "autoprefixer": "^10.4.20",
        "bits-ui": "^1.8.0",
        "clsx": "^2.1.1",
        "dotenv": "^17.2.1",
        "eslint": "^9.7.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.36.0",
        "globals": "^15.0.0",
        "jsdom": "^25.0.1",
        "openapi-fetch": "^0.13.4",
        "openapi-typescript": "^7.4.3",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "storybook": "^8.4.5",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwind-merge": "^2.5.5",
        "tailwind-variants": "^0.3.0",
        "tailwindcss": "^3.4.9",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.0.0",
        "vite": "^5.0.3",
        "vitest": "^2.0.4",
        "vitest-svelte-kit": "^0.0.7"
    },
    "dependencies": {
        "@inlang/paraglide-sveltekit": "^0.11.1",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.15",
        "@tanstack/svelte-query": "^5.65.0",
        "@types/d3-drag": "^3.0.7",
        "apache-arrow": "^21.0.0",
        "d3-drag": "^3.0.0",
        "d3-selection": "^3.0.0",
        "d3-zoom": "^3.0.0",
        "embedding-atlas": "^0.10.0",
        "lodash": "^4.17.21",
        "paneforge": "^1.0.2",
        "posthog-js": "^1.276.0",
        "svelte-infinite-loading": "^1.4.0",
        "svelte-sonner": "^1.0.5",
        "svelte-virtual": "^1.0.0-next.22"
    }
}
